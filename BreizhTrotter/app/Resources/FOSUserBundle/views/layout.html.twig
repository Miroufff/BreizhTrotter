<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        {% block stylesheets %}
            <link href=" {{ asset('../css/master.css') }}" rel="stylesheet" /> 
        {% endblock %}
    </head>
    <body>
        {% include "::navbarTop.html.twig" %}
        
        <div id='user-container' class= "form row">
            <div class="container">
                <div class="row">
                    {% for type, messages in app.session.flashBag.all %}
                        {% for message in messages %}
                            <div class="{{ type }}">
                                {{ message|trans({}, 'FOSUserBundle') }}
                            </div>
                        {% endfor %}
                    {% endfor %}
                </div>
                {% block fos_user_content %}

                {% endblock fos_user_content %}
            </div>
        </div>
    </body>

    <footer>
        {% block javascripts %}
            <link href=" {{ asset('js/bootstrap.min.js') }}" rel="stylesheet" />
        {% endblock %}
    </footer>

</html>