{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Activity</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Day</th>
                <td>{{ entity.day }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Feasibility</th>
                <td>{{ entity.feasibility }}</td>
            </tr>
            <tr>
                <th>Idimage</th>
                <td>{{ entity.idImage }}</td>
            </tr>
            <tr>
                <th>Idscenario</th>
                <td>{{ entity.idScenario }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('ens_activity') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ens_activity_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
