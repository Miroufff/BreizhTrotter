{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Action</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Step</th>
                <td>{{ entity.step }}</td>
            </tr>
            <tr>
                <th>Updatedate</th>
                <td>{{ entity.updateDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Feasibility</th>
                <td>{{ entity.feasibility }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Budget</th>
                <td>{{ entity.budget }}</td>
            </tr>
            <tr>
                <th>Timelimit</th>
                <td>{{ entity.timeLimit }}</td>
            </tr>
            <tr>
                <th>Accomplishment</th>
                <td>{{ entity.accomplishment }}</td>
            </tr>
            <tr>
                <th>image</th>
                <td>{{ entity.image.id }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('ens_action') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ens_action_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
