<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usernameCanonical" type="string" column="username_canonical" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailCanonical" type="string" column="email_canonical" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="salt" type="string" column="salt" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enabled" type="integer" column="enabled" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastLogin" type="date" column="last_login" nullable="false"/>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="locked" type="integer" column="locked" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="expired" type="integer" column="expired" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="expiresAt" type="date" column="expires_at" nullable="false"/>
    <field name="confirmationToken" type="string" column="confirmation_token" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwordRequestedAt" type="date" column="password_requested_at" nullable="false"/>
    <field name="credentialsExpired" type="integer" column="credentials_expired" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="credentialsExpireAt" type="date" column="credentials_expire_at" nullable="false"/>
    <field name="roles" type="string" column="roles" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
