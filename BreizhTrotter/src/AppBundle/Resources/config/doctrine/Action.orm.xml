<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Action" table="action">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="step" type="string" column="step" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updateDate" type="date" column="update_date" nullable="false"/>
    <field name="feasibility" type="integer" column="feasibility" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="budget" type="integer" column="budget" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="timeLimit" type="integer" column="time_limit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="accomplishment" type="integer" column="accomplishment" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idImage" type="integer" column="id_image" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
