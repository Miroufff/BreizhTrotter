<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Constraint" table="constraint">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="updateDate" type="date" column="update_date" nullable="false"/>
    <field name="diffLevel" type="integer" column="diff_level" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="context" type="string" column="context" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="diagAction" type="string" column="diag_action" length="300" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="viabilityImpact" type="integer" column="viability_impact" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tourismImpact" type="integer" column="tourism_impact" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="image" target-entity="Image">
      <join-column name="id_image" referenced-column-name="id" />
    </many-to-one>
  </entity>
</doctrine-mapping>
